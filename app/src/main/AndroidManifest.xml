            <!--android:name=".ReturnParcels.ReturnParcelsActivity"-->
            <!--android:label="Return Parcels"-->
            <!--android:parentActivityName=".MediPackClientActivity"-->
            <!--android:windowSoftInputMode="stateHidden">-->
